<%= form_with model: @dealership, local: true do  |f| %>
<% if @dealership.errors.any? %>
    <div id="error_explanation">
      <h2>There were some errors:</h2>
      <ul>
        <% @dealership.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
Dealership Name: <%= f.text_field :name %><br>
Dealership Type: <%= f.text_field :kind%><br>
Dealership Rating (1-10): <%= f.number_field :rating %><br>
<%=f.submit%>
<%end%>