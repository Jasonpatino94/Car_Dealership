<% if @dealership.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@dealership.errors.count, "error") %>
      prohibited this dealership from being saved:
    </h2>
 
    <ul>
    <% @dealership.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>


<%= form_with model: @dealership do |f| %>
Dealership Name: <%= f.text_field :name %><br>
Dealership Type: <%= f.text_field :kind%><br>
Dealership Rating (1-10): <%= f.number_field :rating %><br>
<%=f.submit%>
<%end%>