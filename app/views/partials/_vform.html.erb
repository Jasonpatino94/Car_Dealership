<% if @vehicle.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@vehicle.errors.count, "error") %>
      prohibited this vehicle from being saved:
    </h2>
 
    <ul>
    <% @vehicle.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>


<%= form_with model: @vehicle do |f| %>
Year: <%= f.number_field :year %><br>
Make: <%= f.text_field :make %><br>
Model <%= f.text_field :model %><br>
Type: <%= f.text_field :kind %><br>
<%=f.submit%>
<%end%>
